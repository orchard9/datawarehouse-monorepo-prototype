{
  "name": "mango",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "complexity": "madge --extensions ts,tsx ./src --summary",
    "circular": "madge --circular --extensions ts,tsx ./src",
    "analyze:size": "vite build --mode analyze",
    "analyze:deps": "madge --extensions ts,tsx ./src --image dependency-graph.svg",
    "check:all": "npm run lint && npm run typecheck && npm run circular",
    "quality": "npm run check:all && npm run build"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "axios": "^1.7.9",
    "lucide-react": "^0.468.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.1.0",
    "recharts": "^2.15.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-bundle-visualizer": "^1.2.1"
  }
}
