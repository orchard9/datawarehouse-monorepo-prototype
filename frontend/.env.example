# Data Warehouse Frontend Environment Variables
# Copy this file to .env.local and configure your settings

# API Configuration
VITE_API_BASE_URL=http://localhost:37951

# Feature Flags
VITE_ENABLE_EXPORT=true
VITE_ENABLE_SYNC_TRIGGER=true
VITE_ENABLE_HEALTH_MONITORING=true

# Cache Settings (milliseconds)
VITE_CACHE_DURATION=300000

# Polling Intervals (milliseconds)
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_SYNC_STATUS_INTERVAL=5000

# Analytics (optional)
VITE_GA_TRACKING_ID=
VITE_ENABLE_ANALYTICS=false

# Debug
VITE_DEBUG_MODE=false